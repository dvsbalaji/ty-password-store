<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>User</servlet-name>
  <servlet-class>com.ty.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>User</servlet-name>
  <url-pattern>/myuser</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>Employee</servlet-name>
  <servlet-class>com.ty.controller.EmployeeController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Employee</servlet-name>
  <url-pattern>/myemployee</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>ValidateUser</servlet-name>
  <servlet-class>com.ty.controller.ValidateUser</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ValidateUser</servlet-name>
  <url-pattern>/validation</url-pattern>
  </servlet-mapping>
</web-app>
